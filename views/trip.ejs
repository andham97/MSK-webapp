    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <script src="http://spin.js.org/spin.min.js"></script>
    <script src="/javascripts/trip.js"></script>
    <script src="/javascripts/tools/site-controller.js"></script>
    <script src="/javascripts/tools/extractDomain.js"></script>
    <script src="/javascripts/tools/notify.js"></script>
    <script src="/javascripts/tools/bootbox.js"></script>
</head>
<body>

<div id="header">
    <div class="logo">
        <a href="/">MSK<span>Tur</span></a>
        <img class="mobile" id="menu" src="/images/icons/menu.png">
    </div>

</div>

<div class="spacer" data-space="55"></div>

<div id="container">

    <div class="sidebar">
        <ul id="nav">
            <li><a href="/"><img src="/images/icons/calendar.png"> Turer<img src="/images/icons/arrow.png" class="arrow"></a></li>
            <li><a href="/badges"><img src="/images/icons/sticker.png"> Merker<img src="/images/icons/arrow.png" class="arrow"></a></li>
            <li><a href="/profile"><img src="/images/icons/profile.png"> Brukerprofil<img src="/images/icons/arrow.png" class="arrow"></a></li>
            <%- menu %>
        </ul>
    </div>

    <div class="content">
        <div id='box'>
            <div class='box-top'>
                <h1 class="noTopMargin" style="margin: 0;"><%- heading %></h1>
            </div>
            <div class='box-panel'>
                <%- trip %>
            </div>
        </div>
        <%- footer %>
    </div>
</div>
</body>
</html>